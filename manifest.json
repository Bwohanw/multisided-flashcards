{
    "name": "Multi-sided Flashcards",
    "description": "Flashcard app with multiple sides, so you can check what you need, when you need.",
    "version": "1.1",
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "16" : "./images/flashcards16.png",
            "32" : "./images/flashcards32.png",
            "48" : "./images/flashcards48.png",
            "128" : "./images/flashcards128.png",
            "256" : "./images/flashcards256.png"
        },
        "default_title": "Multi-Flashcards",
        "default_popup": "templates/index.html"
    }
}